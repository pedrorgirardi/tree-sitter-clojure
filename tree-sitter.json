{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "clojure",
			"camelCase": "Clojure",
      "scope": "source.clojure",
      "path": ".",
      "file-types": ["clj", "cljc", "cljs", "bb"]
    }
  ],
  "metadata": {
    "version": "0.0.13",
    "license": "MIT",
    "description": "Clojure grammar for tree-sitter",
    "authors": [
      {
        "name": "sogaiu"
      }
    ],
    "links": {
      "repository": "https://github.com/sogaiu/tree-sitter-clojure"
    }
  },
  "bindings": {
    "c": false,
    "go": true,
    "java": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
